<form action="/songs" method="post">
  <input type="text" id="Name" name="song[name]">
  <% Genre.all.each do |genre| %>
  <input type="checkbox" id="<%= genre.name %>" name="genres[]" value="<%= genre.id %>">
  <% end %>
  <input type="text" id="Artist Name" name="song[artist]">
  <input type="submit" name="Create" id="Create">

</form>
